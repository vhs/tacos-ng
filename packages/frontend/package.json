{
    "dependencies": {
        "generate-react-cli": "^8.4.8",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "stylelint": "^16.9.0"
    },
    "devDependencies": {
        "@babel/core": "^7.25.2",
        "@babel/preset-env": "^7.25.4",
        "@babel/preset-typescript": "^7.24.7",
        "@chromatic-com/storybook": "^1.9.0",
        "@eslint/js": "^9.9.0",
        "@storybook/addon-essentials": "^8.3.3",
        "@storybook/addon-interactions": "^8.3.3",
        "@storybook/addon-links": "^8.3.3",
        "@storybook/addon-onboarding": "^8.3.3",
        "@storybook/addon-themes": "^8.3.3",
        "@storybook/blocks": "^8.3.3",
        "@storybook/react": "^8.3.3",
        "@storybook/react-vite": "^8.3.3",
        "@storybook/test": "^8.3.3",
        "@testing-library/jest-dom": "^6.5.0",
        "@testing-library/react": "^16.0.1",
        "@types/jest": "29.5.12",
        "@types/react": "^18.3.3",
        "@types/react-dom": "^18.3.0",
        "@vitejs/plugin-basic-ssl": "^1.1.0",
        "@vitejs/plugin-react-swc": "^3.5.0",
        "autoprefixer": "^10.4.20",
        "babel-jest": "^29.7.0",
        "eslint": "^9.9.0",
        "eslint-config-love": "71.0.0",
        "eslint-plugin-react-hooks": "^5.1.0-rc.0",
        "eslint-plugin-react-refresh": "^0.4.9",
        "eslint-plugin-storybook": "^0.8.0",
        "globals": "^15.9.0",
        "identity-obj-proxy": "^3.0.0",
        "jest": "29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "jest-transform-stub": "^2.0.0",
        "postcss": "^8.4.47",
        "prettier": "3.3.3",
        "storybook": "^8.3.3",
        "stylelint-config-recommended": "^14.0.1",
        "stylelint-config-standard": "^36.0.1",
        "stylelint-config-tailwindcss": "^0.0.7",
        "tailwindcss": "^3.4.13",
        "ts-jest": "^29.2.5",
        "typescript": "^5.5.3",
        "typescript-eslint": "^8.0.1",
        "vite": "^5.4.1",
        "vite-tsconfig-paths": "^5.0.1"
    },
    "eslintConfig": {
        "extends": [
            "plugin:storybook/recommended"
        ]
    },
    "name": "@vhs/tacos-frontend",
    "private": true,
    "scripts": {
        "build": "wireit",
        "build-storybook": "storybook build",
        "commit": "pnpm dlx git-cz@latest --scope=frontend",
        "create:component": "generate-react c",
        "dev": "wireit",
        "format": "prettier -w src/**/*.{ts,tsx} src/**/*.{css,scss}",
        "lint": "eslint src/**/*.{ts,tsx} && stylelint src/**/*.{css,scss}",
        "lint:fix": "eslint --fix src/**/*.{ts,tsx} && stylelint --fix src/**/*.{css,scss}",
        "preview": "wireit",
        "storybook": "wireit",
        "test": "jest"
    },
    "type": "module",
    "version": "1.0.0",
    "wireit": {
        "build": {
            "command": " vite build",
            "dependencies": [
                "build:composite"
            ]
        },
        "build:composite": {
            "command": "tsc -b"
        },
        "dev": {
            "command": "vite",
            "service": true
        },
        "preview": {
            "command": "vite preview",
            "service": true
        },
        "storybook": {
            "command": "storybook dev -p 6006",
            "service": true
        }
    }
}
